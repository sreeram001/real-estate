{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "constructor-super": 2,
    "getter-return": 2,
    "no-class-assign": 2,
    "no-cond-assign": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-dupe-class-members": 2,
    "no-ex-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-misleading-character-class": 2,
    "no-new-symbol": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-prototype-builtins": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-labels": 2,
    "require-atomic-updates": 1,
    "require-yield": 2,
    "strict": "warn",
    "use-isnan": 2,
    "no-self-compare": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-self-assign": 2,
    "no-empty-pattern": 2,
    "no-dupe-args": 2,
    "no-dupe-else-if": 2,
    "no-debugger": "warn",
    "max-lines": "off",
    "no-array-constructor": 2,
    "max-len": [
      "error",
      {
        "code": 300
      }
    ],
    "space-before-function-paren": [
      0,
      "always"
    ],
    "no-trailing-spaces": "error",
    "arrow-spacing": "error",
    "no-extra-boolean-cast": 2,
    "no-func-assign": 2,
    "no-global-assign": "error",
    "no-eval": 2,
    "array-callback-return": "error",
    "no-implicit-globals": "error",
    "no-useless-catch": 2,
    "no-with": 2,
    "valid-typeof": 2,
    "default-param-last": 2,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "prefer-const": 2,
    "class-methods-use-this": [
      0
    ],
    "no-undef": [
      1
    ],
    "no-case-declarations": [
      1
    ],
    "no-return-assign": [
      1
    ],
    "no-param-reassign": [
      2
    ],
    "camelcase": [
      0
    ],
    "no-underscore-dangle": [
      0,
      "always"
    ],
    "no-fallthrough": 2,
    "no-var": [
      2
    ],
    "no-unused-vars": [
      2
    ],
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-duplicate-imports": 2,
    "prefer-destructuring": 2,
    "no-loop-func": 2,
    "for-direction": 2,
    "no-async-promise-executor": 0,
    "no-await-in-loop": "off",
    "no-compare-neg-zero": 2,
    "init-declarations": 0,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-restricted-globals": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undefined": 0,
    "no-restricted-exports": 2,
    "no-restricted-imports": 2,
    "object-shorthand ": 0,
    "operator-assignment": 0,
    "padding-line-between-statements": 2,
    "prefer-exponentiation-operator": 2,
    "prefer-object-spread": 2,
    "quote-props": 0,
    "quotes": 0,
    "semi": 2, //semi
    "no-alert": 1,
    // require the use of === and !==
    "eqeqeq": 2,
    "no-div-regex": 2,
    "block-scoped-var": 2,
    "dot-notation": [
      0,
      {
        "allowKeywords": true
      }
    ],
    // require default case in switch statements
    "default-case": 2,
    "no-console": 1,
    "yield-star-spacing": [
      "error",
      "both"
    ],
    "wrap-regex": 2,
    "unicode-bom": 2,
    "template-tag-spacing": [
      "error",
      "always"
    ],
    "switch-colon-spacing": "error",
    "space-unary-ops": "error",
    "space-in-parens": "error",
    "space-before-blocks": [
      "error",
      "always"
    ],
    "semi-style": [
      "error",
      "last"
    ],
    "semi-spacing": "error",
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "no-tabs": "error",
    "no-multi-spaces": [
      "error",
      {
        "ignoreEOLComments": false
      }
    ],
    "no-mixed-spaces-and-tabs": [
      "error",
      "smart-tabs"
    ],
    "no-useless-escape": "off",
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "key-spacing": [
      "error",
      {
        "afterColon": true
      }
    ],
    "generator-star-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "eol-last": [
      "error",
      "always"
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "comma-spacing": [
      "error",
      {
        "before": false,
        "after": true
      }
    ],
    "arrow-parens": [
      "error",
      "always"
    ],
    "array-bracket-spacing": "error",
    "yoda": "error",
    "require-await": "error"
  }
}